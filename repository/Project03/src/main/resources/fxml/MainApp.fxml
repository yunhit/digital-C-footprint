<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.HomeController">
    <ImageView fx:id="mainImageView" fitWidth="600" fitHeight="600" preserveRatio="true" AnchorPane.topAnchor="50.0" AnchorPane.leftAnchor="50.0"/>
    
    <Button fx:id="treeButton1" prefWidth="50" prefHeight="50" style="-fx-rotate: 45;" AnchorPane.topAnchor="190.0" AnchorPane.leftAnchor="250.0" onAction="#plantTree"/>
    <Button fx:id="treeButton2" prefWidth="50" prefHeight="50" style="-fx-rotate: 45;" AnchorPane.topAnchor="220.0" AnchorPane.leftAnchor="205.0" onAction="#plantTree"/>
    <Button fx:id="treeButton3" prefWidth="50" prefHeight="50" style="-fx-rotate: 45;" AnchorPane.topAnchor="220.0" AnchorPane.leftAnchor="295.0" onAction="#plantTree"/>
    
    <Button fx:id="treeButton4" prefWidth="50" prefHeight="50" style="-fx-rotate: 45;" AnchorPane.topAnchor="245.0" AnchorPane.leftAnchor="155.0" onAction="#plantTree"/>
    <Button fx:id="treeButton5" prefWidth="50" prefHeight="50" style="-fx-rotate: 45;" AnchorPane.topAnchor="245.0" AnchorPane.leftAnchor="250.0" onAction="#plantTree"/>
    <Button fx:id="treeButton6" prefWidth="50" prefHeight="50" style="-fx-rotate: 45;" AnchorPane.topAnchor="245.0" AnchorPane.leftAnchor="345.0" onAction="#plantTree"/>
    
    <Button fx:id="treeButton7" prefWidth="50" prefHeight="50" style="-fx-rotate: 45;" AnchorPane.topAnchor="275.0" AnchorPane.leftAnchor="200.0" onAction="#plantTree"/>
    <Button fx:id="treeButton8" prefWidth="50" prefHeight="50" style="-fx-rotate: 45;" AnchorPane.topAnchor="275.0" AnchorPane.leftAnchor="300.0" onAction="#plantTree"/>
    <Button fx:id="treeButton9" prefWidth="50" prefHeight="50" style="-fx-rotate: 45;" AnchorPane.topAnchor="310.0" AnchorPane.leftAnchor="250.0" onAction="#plantTree"/>

    <HBox spacing="20" alignment="center" prefHeight="200" prefWidth="1200" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0">
        <Button fx:id="worldButton" prefHeight="100" prefWidth="100">
            <graphic>
                <ImageView fx:id="worldIcon" fitHeight="115" fitWidth="115" translateX="-330" translateY="-30"/>
            </graphic>
        </Button>
        <Button fx:id="missionsButton" prefHeight="100" prefWidth="100">
            <graphic>
                <ImageView fx:id="missionsIcon" fitHeight="115" fitWidth="115" translateX="-330" translateY="-30"/>
            </graphic>
        </Button>
        <Button fx:id="reportButton" prefHeight="100" prefWidth="100">
            <graphic>
                <ImageView fx:id="reportIcon" fitHeight="115" fitWidth="115" translateX="-330" translateY="-30"/>
            </graphic>
        </Button>
    </HBox>

    <Label fx:id="seedLabel" text="30" style="-fx-font-size: 35;" AnchorPane.topAnchor="80.0" AnchorPane.rightAnchor="150.0"/>
</AnchorPane>
